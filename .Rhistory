pivot_longer(-country, names_to="year",values_to="cases") #-country means all the columns except country
table4b %>%
pivot_longer(-country, names_to="year",values_to="population")
full_join(table4a,table4b)
table4a %>%
pivot_longer(-country, names_to="year",values_to="cases") #-country means all the columns except country
